#big o- time complexity and space complexity examples

"""1. worst case"""
# factorial of given number
def factorial(n):
    if n < 0:
        return 0
    elif n == 0 or n == 1:
        return 1
    else:
        fact = 1
        while(n > 1): #O(n)
            fact *= n #O(n)
            n -= 1 #O(n)
        return fact
 num = 5;
print("Factorial of",num,"is",
factorial(num)) #O(n+n+n)=O(3n)=O(n)
